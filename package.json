  "main": "dist/index.js",
  "devDependencies": {
    "@types/node": "20.5.3",
    "@types/readable-stream": "4.0.1",
    "@types/ws": "8.5.5",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "eslint": "8.47.0",
    "homebridge": "1.6.1",
    "nodemon": "3.0.1",
    "rimraf": "5.0.1",
    "typescript": "5.1.6"
